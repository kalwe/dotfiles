Urxvt*font:             xft:Bitstream Vera Sans Mono:size=12
Urxvt*termName:         rxvt

URxvt.scrollBar:        false

Urxvt*perl-lib:         /usr/lib/urxvt/perl/
Urxvt*perl-ext-common:  default,matcher
Urxvt*urlLauncher:      /usr/bin/jumanji
Urxvt*matcher.button:   1

! do not scroll to bottum when tty receives output
URxvt*scrollTtyOutput: false

!URxvt.url-launcher: /usr/bin/firefox

!urxvt*fading:           50
!urxvt*fadeColor:        #000000

URxvt*saveLines: 65535

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

Rxvt.transparent:   true
URxvt.tintColor:     white
URxvt.shading:     40 

! Background color
URxvt*background: black 

! Font color
URxvt*foreground: green 

URxvt*pointerColorBackground:	red
URxvt*pointerColorForeground:	green

! Other colors
URxvt*color0: black
URxvt*color1: red3
URxvt*color2: green3
URxvt*color3: yellow3
URxvt*color4: blue2
URxvt*color5: magenta3
URxvt*color6: cyan3
URxvt*color7: gray90
URxvt*color8: grey50
URxvt*color9: red
URxvt*color10: green
URxvt*color11: yellow
URxvt*color12: blue
URxvt*color13: magenta
URxvt*color14: cyan
URxvt*color15: white
